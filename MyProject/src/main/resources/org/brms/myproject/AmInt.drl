when
AmInt((flatRoofPercent=="1 - 10%")||(flatRoofPercent=="11 - 20%"))
then
System.out.println("Flat roof percentage eligible");
drools.setFocus("Carrier");
end

rule "Check masonary frame ratio"
ruleflow-group "Carrier"
